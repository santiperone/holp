{
  "name": "holp",
  "version": "1.0.2",
  "description": "HOF approach for AWS Lambda proxy integrations",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "require": "./dist/errors.cjs",
      "types": "./errors.d.ts"
    }
  },
  "files": [
    "dist/*",
    "*.d.ts",
    "*.d.mts"
  ],
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.3.0",
    "@total-typescript/shoehorn": "^0.1.1",
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "scripts": {
    "build:vite": "vite build",
    "build:ts": "npx tsc -p tsconfig.json",
    "build": "npm run build:vite && npm run build:ts",
    "lint": "eslint ./src/**/*.ts -c .eslintrc.json",
    "test": "jest --silent --coverage"
  },
  "author": "Santiago Perone",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
