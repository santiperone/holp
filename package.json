{
  "name": "holp",
  "version": "1.0.2",
  "description": "HOF approach for AWS Lambda proxy integrations",
  "repository": {
    "type": "git",
    "url": "https://github.com/santiperone/holp"
  },
  "main": "./dist-cjs/index.js",
  "module": "./dist-esm/index.js",
  "types": "./dist-cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.js"
    },
    "./errors": {
      "import": "./dist-esm/errors/index.js",
      "require": "./dist-cjs/errors/index.js"
    }
  },
  "files": [
    "dist-esm/*",
    "dist-cjs/*",
    "*.d.ts",
    "*.d.mts"
  ],
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.3.0",
    "@total-typescript/shoehorn": "^0.1.1",
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint ./src/**/*.ts -c .eslintrc.json",
    "attw": "attw holp-$npm_package_version.tgz",
    "test": "jest --silent --coverage"
  },
  "author": "Santiago Perone",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
