{
  "name": "holp",
  "version": "1.0.4",
  "description": "HOF approach for AWS Lambda proxy integrations",
  "repository": {
    "type": "git",
    "url": "https://github.com/santiperone/holp"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.mts"
    },
    "./errors": {
      "import": "./dist/errors/index.mjs",
      "require": "./dist/errors/index.js",
      "types": "./dist/errors/index.d.mts"
    }
  },
  "files": [
    "dist/*",
    "*.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.4.2",
    "@total-typescript/shoehorn": "^0.1.1",
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint ./src/**/*.ts -c .eslintrc.json",
    "attw": "attw --pack",
    "test": "jest --silent --coverage",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --max-warnings=0 --fix"
    ]
  },
  "author": "Santiago Perone",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
